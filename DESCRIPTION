Package: CAMPSITE
Title: Competition And Multiple-Peak Selection Integrated Trait Evolution
Version: 0.0.0.9000
Authors@R: 
    c(person("Alex", "Slavenko", , "alex.slavenko1@gmail.com", role = c("aut", "cre"),
           comment = c(ORCID = "0000-0002-3265-7715")),
      person("Anna", "Krystalli", , "annakrystalli@googlemail.com", role = "aut",
